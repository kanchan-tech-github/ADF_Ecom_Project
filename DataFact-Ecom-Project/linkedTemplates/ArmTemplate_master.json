{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFact-Ecom-Project"
		},
		"LS_for_all_Parquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_for_all_Parquet'"
		},
		"LS_RestService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retoolapi.dev/STkIEh/data"
		},
		"LS_for_all_Parquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls0staging0ecom.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFact-Ecom-Project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_for_all_Parquet_accountKey": {
						"value": "[parameters('LS_for_all_Parquet_accountKey')]"
					},
					"LS_RestService_properties_typeProperties_url": {
						"value": "[parameters('LS_RestService_properties_typeProperties_url')]"
					},
					"LS_for_all_Parquet_properties_typeProperties_url": {
						"value": "[parameters('LS_for_all_Parquet_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}